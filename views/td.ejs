<!-- td -->
<div>
    <% if (products.length) { %> 
        <table class="table">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Price</th>
                <th scope="col">Image</th>
              </tr>
            </thead>
            <tbody>
                <% products.forEach((product, products) => { %> 

              <tr>
                <th scope="row"><%= home + 1 %> </th>
                <td><%- product.id %> </td>
                <td><%- product.name %> </td>
                <td><%- product.price %> </td>
                <td><img src="<%= product.img %> " width="56"/></td>
              </tr>
              <% }) %> 
            </tbody>
          </table>
        <% } else { %> 
            Data Not Found 
            <% } %> 
</div>